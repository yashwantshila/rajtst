{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "/blog/**",
        "destination": "/blog/index.html"
      }
    ],
    "headers": [],
    "redirects": [],
    "cleanUrls": true

  },
  "storage": {
    "rules": "storage.rules"
  }
}
